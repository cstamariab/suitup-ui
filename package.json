  "repository": {
    "type": "git",
    "url": "git+https://github.com/erasmo-marin/suitup-ui.git"
  },
  "files": [
    "dist",
    "lib"
  ],
  "keywords": [
    "suitup",
    "ui",
    "framework",
    "toolkit",
    "react",
    "uikit",
    "webapp"
  ],
