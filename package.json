{
  "name": "suitup-ui",
  "version": "0.0.1",
  "description": "Suitup UI Toolkit is a set of React components to build web apps faster",
  "main": "lib/index.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "less": "lessc --clean-css --autoprefix='last 2 versions' src/styles/index.less dist/suitup-toolkit.min.css",
    "babel": "babel src/components/ -d lib/",
    "build": "npm run babel && webpack --progress --colors && npm run less",
    "watch": "webpack --progress --colors --watch",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "debug": "~2.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.14.1",
    "react": "^15.2.0",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.2.0",
    "react-draggable": "^2.2.3",
    "react-highlight": "^0.9.0",
    "react-notify-resize": "^2.0.4",
    "react-router": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.22.0",
    "body-parser": "~1.15.1",
    "chalk": "^1.1.3",
    "cookie-parser": "~1.4.3",
    "css-loader": "^0.26.1",
    "express": "~4.13.4",
    "express-session": "^1.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "hbs": "~4.0.0",
    "jsome": "^2.3.25",
    "jsx-control-statements": "^3.1.5",
    "jsx-to-string": "^1.0.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "morgan": "~1.7.0",
    "node-uuid": "^1.4.7",
    "react-hot-loader": "^3.0.0-beta.6",
    "request": "^2.72.0",
    "serve-favicon": "~2.3.0",
    "style-loader": "^0.13.1",
